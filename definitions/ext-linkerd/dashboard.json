{
  "name": "Linkerd",
  "description": null,
  "pages": [
    {
      "name": "Linkerd",
      "description": null,
      "widgets": [
        {
          "title": "Total Requests",
          "layout": {
            "column": 1,
            "row": 3,
            "width": 4,
            "height": 2
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "from Metric SELECT count(request_total) as 'Requests' where instrumentation.source = 'Linkerd_Prometheus' TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "title": "Tota Responses",
          "layout": {
            "column": 5,
            "row": 3,
            "width": 4,
            "height": 2
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT count(response_total) as 'Responses' from Metric where instrumentation.source = 'Linkerd_Prometheus' and response_total IS NOT NULL TIMESERIES AUTO "
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        },
        {
          "title": "Response Latency",
          "layout": {
            "column": 9,
            "row": 3,
            "width": 4,
            "height": 2
          },
          "linkedEntityGuids": null,
          "visualization": {
            "id": "viz.line"
          },
          "rawConfiguration": {
            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT average(response_latency_ms_sum) as 'Latency' from Metric where instrumentation.source = 'Linkerd_Prometheus' TIMESERIES AUTO"
              }
            ],
            "platformOptions": {
              "ignoreTimeRange": false
            },
            "units": {
              "unit": "MS"
            },
            "yAxisLeft": {
              "zero": true
            }
          }
        }
      ]
    }
  ]
}