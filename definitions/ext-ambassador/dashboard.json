            "facet": {
              "showOtherSeries": false
            },
            "legend": {
              "enabled": true
            },
            "nrqlQueries": [
              {
                "accountId": 0,
                "query": "SELECT latest(envoy_http_downstream_cx_http_active) FROM Metric where endpoint ='ambassador-admin' TIMESERIES AUTO"
